{
  "name": "citizen-func",
  "version": "5.5.9",
  "license": "UNLICENSED",
  "type": "commonjs",
  "scripts": {
    "build": "yarn openapi:bundle && yarn generate && tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "openapi:bundle": "yarn node copy-commons-definitions.js && bundle-api-spec -i openapi/index.yaml.template -o openapi/index.yaml -V $npm_package_version",
    "dist:modules": "modclean -r -n default:safe && yarn install --production",
    "version": "yarn openapi:bundle",
    "lint": "eslint --fix src",
    "lint:check": "eslint src",
    "generate": "yarn generate:definitions",
    "generate:definitions": "shx rm -rf src/generated/definitions && shx mkdir -p src/generated/definitions && gen-api-models --api-spec ./openapi/index.yaml --no-strict --out-dir ./src/generated/definitions",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "start": "func start --port 7072"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "^4.0.3",
    "@pagopa/openapi-codegen-ts": "^14.0.0",
    "@types/express": "^4.17.11",
    "@types/html-to-text": "^1.4.31",
    "@types/node": "^20.17.30",
    "@types/redis": "^2.8.28",
    "@vitest/coverage-v8": "^3.1.2",
    "eslint": "8.57.1",
    "modclean": "^3.0.0-beta.1",
    "prettier": "3.2.5",
    "rimraf": "^6.0.1",
    "shx": "^0.3.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@azure/functions": "^3.2.0",
    "@azure/identity": "^4.5.0",
    "@pagopa/azure-storage-legacy-migration-kit": "^0.0.4",
    "@pagopa/express-azure-functions": "^2.0.0",
    "@pagopa/io-functions-commons": "^29.3.0",
    "@pagopa/ts-commons": "^11.0.0",
    "applicationinsights": "^1.8.10",
    "azure-storage": "^2.10.3",
    "express": "^4.15.3",
    "fp-ts": "^2.10.5",
    "io-ts": "^2.2.16",
    "redis": "^4.6.15"
  }
}
