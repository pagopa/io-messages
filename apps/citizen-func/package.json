{
  "name": "citizen-func",
  "version": "5.5.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn openapi:bundle && rimraf ./generated && yarn generate:definitions",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "openapi:bundle": "bundle-api-spec -i openapi/index.yaml.template -o openapi/index.yaml -V $npm_package_version",
    "generate:definitions": "shx rm -rf generated/definitions && shx mkdir -p generated/definitions && gen-api-models --api-spec ./openapi/index.yaml --no-strict --out-dir ./generated/definitions",
    "deploy": "npm run build && func azure functionapp publish agid-functions-app-test",
    "dist:modules": "modclean -r -n default:safe && yarn install --production",
    "predeploy": "npm-run-all build dist:modules",
    "preversion": "auto-changelog  --config .auto-changelog.json --unreleased --commit-limit false --stdout --template preview.hbs",
    "version": "yarn bump:openapi && yarn bump:changelog",
    "bump:openapi": "bundle-api-spec -i openapi/index.yaml -o openapi/index.yaml -V $npm_package_version && git add openapi/index.yaml",
    "bump:changelog": "auto-changelog -p --config .auto-changelog.json --unreleased && git add CHANGELOG.md"
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "3.2.0",
    "@pagopa/eslint-config": "^1.3.1",
    "@pagopa/openapi-codegen-ts": "^10.0.5",
    "@types/express": "^4.17.11",
    "@types/html-to-text": "^1.4.31",
    "@types/jest": "^29.5.12",
    "@types/lolex": "^3.1.1",
    "@types/redis": "^2.8.28",
    "auto-changelog": "^2.2.1",
    "dotenv": "^8.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-check": "^1.16.0",
    "jest": "^29.7.0",
    "lolex": "^5.1.1",
    "modclean": "^3.0.0-beta.1",
    "nock": "^13.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "shx": "^0.3.2",
    "ts-jest": "^29.2.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@azure/storage-queue": "^12.4.0",
    "@pagopa/express-azure-functions": "^2.0.0",
    "@pagopa/io-functions-commons": "^29.1.1",
    "@pagopa/ts-commons": "^11.0.0",
    "abort-controller": "^3.0.0",
    "applicationinsights": "^1.8.10",
    "azure-storage": "^2.10.3",
    "date-fns": "^2.16.1",
    "dependency-check": "^4.1.0",
    "express": "^4.15.3",
    "fp-ts": "^2.10.5",
    "html-to-text": "^5.1.1",
    "io-ts": "^2.2.16",
    "node-fetch": "^2.6.1",
    "redis": "^4.6.15"
  }
}
