{
  "version": "2.0",
  "functionTimeout": "02:30:00",
  "functions": ["CosmosApiMessagesChangeFeed"],
  "logging": {
    "logLevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5,
        "includedTypes": "PageView;Trace;Dependency;Request",
        "excludedTypes": "Exception;Event;CustomEvent"
      }
    }
  },
  "extensions": {
    "cosmosDB": {
      "connectionMode": "Gateway",
      "protocol": "Https"
    },
    "eventHubs": {
      "initialOffsetOptions": {
          "type": "fromStart",
          "enqueuedTimeUtc": ""
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  }
}
