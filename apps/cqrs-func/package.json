{
  "name": "cqrs-func",
  "author": "IO team",
  "version": "1.0.6",
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn generate && tsc",
    "start": "func start --javascript",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "generate:avro": "shx rm -rf ./generated/avro && shx mkdir -p ./generated/avro && avro-to-typescript --compile ./avro ./generated/avro",
    "generate:payment-updater": "shx rm -rf generated/payment-updater && shx mkdir -p generated/payment-updater && gen-api-models --api-spec ./openapi/payment-updater.yaml --no-strict --out-dir ./generated/payment-updater --request-types --response-decoders --client",
    "generate": "yarn generate:payment-updater && yarn generate:avro",
    "dist:modules": "modclean -r -n default:safe && yarn install --production"
  },
  "devDependencies": {
    "@azure/functions": "^3.2.0",
    "@pagopa/danger-custom-rules": "^2.0.3",
    "@pagopa/eslint-config": "^1.3.1",
    "@pagopa/openapi-codegen-ts": "^11.1.0",
    "@types/express": "^4.17.12",
    "@types/lodash": "^4.14.173",
    "@types/node": "^20.17.12",
    "@types/node-fetch": "^2.5.7",
    "@types/vfile-message": "^2.0.0",
    "auto-changelog": "2.2.1",
    "danger": "^10.3.0",
    "dependency-check": "^4.1.0",
    "dotenv-cli": "^3.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "modclean": "^3.0.0-beta.1",
    "npm-run-all": "^4.1.5",
    "oval": "^1.0.0",
    "prettier": "^1.18.2",
    "shx": "^0.3.2",
    "typescript": "^4.3.5",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@azure/identity": "^4.5.0",
    "@azure/storage-queue": "^12.8.0",
    "@chasdevs/avro-to-typescript": "^1.4.0",
    "@pagopa/express-azure-functions": "^2.1.1",
    "@pagopa/fp-ts-kafkajs": "^0.3.0",
    "@pagopa/io-functions-commons": "^29.0.4",
    "@pagopa/ts-commons": "^11.0.0",
    "applicationinsights": "^1.7.4",
    "avsc": "^5.7.3",
    "azure-storage": "^2.10.5",
    "eslint-plugin-perfectionist": "^2.9.0",
    "express": "^4.15.3",
    "fp-ts": "^2.10.5",
    "io-ts": "^2.2.16",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "winston": "^3.2.1"
  },
  "resolutions": {
    "handlebars": "~4.5.3"
  }
}
