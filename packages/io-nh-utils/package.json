{
  "name": "io-nh-utils",
  "version": "1.0.0",
  "author": "",
  "license": "ISC",
  "description": "",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dotnet:build": "dotnet build -c Release ./src/NhRegJobs/NhRegJobs.csproj",
    "dotnet:restore": "dotnet restore ./src/NhRegJobs/NhRegJobs.csproj",
    "nh:export": "dotenv -e .env -- dotnet run -c Release --project ./src/NhRegJobs/NhRegJobs.csproj -- export",
    "import": "tsx ./src/import.ts",
    "clean": "tsx ./src/clean.ts",
    "compare": "tsx ./src/compare.ts",
    "send": "tsx ./src/send.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix src",
    "lint:check": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@azure/notification-hubs": "^1.2.3",
    "@azure/storage-blob": "^12.26.0",
    "@pagopa/eslint-config": "^4.0.3",
    "commander": "^14.0.1",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^5.3.0",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "8.57.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
